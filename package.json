{
  "name": "webpack-playground",
  "version": "1.0.0",
  "description": "testing current state webpack-react stack",
  "main": "index.js",
  "repository": "git@bitbucket.org:bnowak/webpack-playground.git",
  "scripts": {
    "build": "webpack --env production --config './.webpack/webpack.config.js' --progress",
    "lint:js": "eslint . --cache --ignore-path .gitignore",
    "lint:css": "stylelint app/**/*.css",
    "start:simple": "webpack-dev-server --env development --config './.webpack/webpack.config.js' --open",
    "start": "webpack-dashboard -- webpack-dev-server --env development  --config './.webpack/webpack.config.js' --open",
    "test": "nyc ava",
    "stories": "start-storybook -p 9001 -c ./.storybook",
    "test:watch": "ava --watch"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@storybook/react": "^3.2.11",
    "autoprefixer": "^7.1.3",
    "ava": "^0.22.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-define": "^1.3.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "babili-webpack-plugin": "^0.1.2",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.5",
    "cssnano": "^3.10.0",
    "dotenv": "^4.0.0",
    "duplicate-package-checker-webpack-plugin": "^1.2.5",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-15": "^1.0.1",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "fbjs": "^0.8.16",
    "file-loader": "^0.11.2",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "git-revision": "0.0.2",
    "glob": "^7.1.2",
    "history": "^3.3.0",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "nyc": "^11.2.1",
    "optimize-css-assets-webpack-plugin": "^3.1.1",
    "postcss": "^6.0.10",
    "postcss-loader": "^2.0.6",
    "prop-types": "^15.6.0",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "^5.0.6",
    "react-router": "^3.0",
    "react-router-redux": "^4.0",
    "react-test-renderer": "^15.6.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.15.6",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "stylelint": "^8.0.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-webpack-plugin": "^0.9.0",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dashboard": "^1.0.0-5",
    "webpack-dev-server": "^2.7.1",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "purecss": "^1.0.0",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "when-dom-ready": "^1.2.12"
  },
  "ava": {
    "files": [
      "app/**/*.test.js"
    ],
    "require": [
      "babel-register",
      "./test/helpers/setupBrowserEnv.js"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 85,
    "statements": 85,
    "functions": 85,
    "branches": 85,
    "reporter": [
      "lcov",
      "html",
      "text-summary"
    ],
    "cache": true,
    "report-dir": "./reports/coverage"
  }
}
