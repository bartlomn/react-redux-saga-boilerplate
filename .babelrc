{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": [
            "> 1%",
            "Last 2 versions",
            "IE 8"
          ]
        },
        "useBuiltIns": true
      }
    ],
    "es2015",
    "react"
  ],
  "plugins": [
    ["transform-define", "app/config/defineVariables.js"]
  ]
}
